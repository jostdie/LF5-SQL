DROP DATABASE IF EXISTS `KrautUndRueben`;
CREATE DATABASE `KrautUndRueben`;

CREATE TABLE `KrautUndRueben`.`Kunde` (
    `KundenNr` INT NOT NULL , 
    `Name` TEXT NOT NULL , 
    `Email` TEXT , 
    `Geburtsdatum` DATE ,
    `Telefon` VARCHAR(20) , 
    PRIMARY KEY (`KundenNr`) 
) ENGINE = InnoDB;
                                       
CREATE TABLE `KrautUndRueben`.`Adresse` (
    `Adress-ID` INT NOT NULL AUTO_INCREMENT,
    `Ort-ID` INT NOT NULL,
    `LieferantenNr` INT NULL, 
    `Strasse` TEXT , 
    `Hausnummer` VARCHAR(9) , 
    PRIMARY KEY (`Adress-ID`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Ort` (
    `Ort-ID` INT NOT NULL AUTO_INCREMENT,
    `PLZ` VARCHAR(5) NOT NULL, 
    `Stadt` TEXT NOT NULL, 
    PRIMARY KEY (`Ort-ID`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Lieferant` (
    `LieferantenNr` INT NOT NULL ,
    `Name` TEXT NOT NULL ,
    `Telefon` VARCHAR(20) NOT NULL ,
    `Email`TEXT NOT NULL , 
    PRIMARY KEY (`LieferantenNr`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Bestellung` (
    `BestellNr` INT NOT NULL ,
    `KundenNr`INT NOT NULL , 
    `Bestelldatum` DATE NOT NULL ,
    `Rechnungsbetrag` DECIMAL(5,2) NOT NULL ,
    `Waehrung` VARCHAR(3) DEFAULT 'EUR' , 
    PRIMARY KEY (`BestellNr`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Zutat` (
    `ZutatNr` INT NOT NULL , 
    `Bezeichnung` TEXT NOT NULL , 
    `Nettopreis (EUR)` DECIMAL(5,2) NOT NULL ,
    `Bruttopreis (EUR)` DECIMAL(5,2) NOT NULL ,
    `Bestand` VARCHAR(5) NOT NULL ,
    `Mengeneinheit` TEXT NOT NULL ,
    `CO2-Emission` DECIMAL(5,3) NOT NULL COMMENT 'pro 100g' , 
    PRIMARY KEY (`ZutatNr`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Ernaehrungskategorien` (
    `KategorieNr` INT NOT NULL , 
    `Bezeichnung` TEXT NOT NULL, 
    PRIMARY KEY (`KategorieNr`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Allergene` (
    `AllergenNr` INT NOT NULL , 
    `Allergen` TEXT NOT NULL, 
    PRIMARY KEY (`AllergenNr`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Naehrstoffangaben` (
    `Naehrstoff-ID` INT NOT NULL AUTO_INCREMENT, 
    `ZutatNr` INT,
    `RezeptNr` INT,
    `Naehrstoffangaben` TEXT DEFAULT 'pro 100g', 
    `Kalorien` VARCHAR(20) NOT NULL,
    `Protein` DECIMAL(4,2) NOT NULL, 
    `Kohlenhydrate` DECIMAL(4,2) NOT NULL,
    `- davon Zucker` DECIMAL(4,2) NOT NULL, 
    `Fett`DECIMAL(5,2) NOT NULL, 
    `- davon gesättigte Fettsäuren` DECIMAL(4,2) NOT NULL, 
    `Ballaststoffe` DECIMAL(4,2) NOT NULL,
    `Natrium` DECIMAL(6,2) NOT NULL, 
    PRIMARY KEY(`Naehrstoff-ID`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Rezept` (
    `RezeptNr` INT NOT NULL,
    `Rezept` TEXT NOT NULL, 
    `CO2-Emission` DECIMAL(5,3),
    PRIMARY KEY(`RezeptNr`) 
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Bestellung-Zutat` (
    `BestellNr` INT NOT NULL ,
    `ZutatNr` INT NOT NULL , 
    `Menge` DECIMAL(5,2) NOT NULL,
    `Mengenangabe` TEXT NOT NULL
) ENGINE = InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Zutat-Ernaehrungskategorie` (
    `ZutatNr` INT NOT NULL , 
    `KategorieNr` INT NOT NULL
) ENGINE=InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Zutat-Lieferant` (
    `ZutatNr` INT NOT NULL ,
    `LieferantenNr` INT NOT NULL , 
    `Einkaufspreis (EUR)` decimal(10,2) NOT NULL 
) ENGINE=InnoDB;
                                         
CREATE TABLE `KrautUndRueben`.`Zutat-Rezept` (
    `ZutatNr` INT NOT NULL , 
    `RezeptNr` INT NOT NULL ,
    `Menge` decimal(10,2) NOT NULL,
    `Mengeneinheit` TEXT,
    `Hinweis` TEXT
) ENGINE=InnoDB;

CREATE TABLE `KrautUndRueben`.`Zutat-Allergene` (
    `ZutatNr` INT NOT NULL ,
    `AllergenNr` INT NOT NULL
) ENGINE=InnoDB;

CREATE TABLE `KrautUndRueben`.`Kunde-Adresse` (
    `KundenNr`INT NOT NULL, 
    `Adress-ID` int (11) NOT NULL
) ENGINE=InnoDB;

USE krautundrueben;

ALTER TABLE Adresse ADD FOREIGN KEY (LieferantenNr) REFERENCES Lieferant (LieferantenNr);
ALTER TABLE Adresse ADD FOREIGN KEY (`Ort-ID`) REFERENCES Ort (`Ort-ID`);

ALTER TABLE `Kunde-Adresse` ADD FOREIGN KEY (KundenNr) REFERENCES Kunde (KundenNr);
ALTER TABLE `Kunde-Adresse` ADD FOREIGN KEY (`Adress-ID`) REFERENCES Adresse (`Adress-ID`);

ALTER TABLE Naehrstoffangaben ADD FOREIGN KEY (ZutatNr) REFERENCES Zutat (ZutatNr);
ALTER TABLE Naehrstoffangaben ADD FOREIGN KEY (RezeptNr) REFERENCES Rezept (RezeptNr);

ALTER TABLE Bestellung ADD FOREIGN KEY (KundenNr) REFERENCES Kunde (KundenNr);

ALTER TABLE `Bestellung-Zutat` ADD FOREIGN KEY (BestellNr) REFERENCES Bestellung (BestellNr);
ALTER TABLE `Bestellung-Zutat` ADD FOREIGN KEY (ZutatNr) REFERENCES Zutat (ZutatNr);

ALTER TABLE `Zutat-Ernaehrungskategorie` ADD FOREIGN KEY (ZutatNr) REFERENCES Zutat (ZutatNr);
ALTER TABLE `Zutat-Ernaehrungskategorie` ADD FOREIGN KEY (KategorieNr) REFERENCES Ernaehrungskategorien (KategorieNr);

ALTER TABLE `Zutat-Allergene` ADD FOREIGN KEY (ZutatNr) REFERENCES Zutat (ZutatNr);
ALTER TABLE `Zutat-Allergene` ADD FOREIGN KEY (AllergenNr) REFERENCES Allergene (AllergenNr);

ALTER TABLE `Zutat-Rezept` ADD FOREIGN KEY (ZutatNr) REFERENCES Zutat (ZutatNr);
ALTER TABLE `Zutat-Rezept` ADD FOREIGN KEY (RezeptNr) REFERENCES Rezept (RezeptNr);

ALTER TABLE `Zutat-Lieferant` ADD FOREIGN KEY (ZutatNr) REFERENCES Zutat (ZutatNr);
ALTER TABLE `Zutat-Lieferant` ADD FOREIGN KEY (LieferantenNr) REFERENCES Lieferant (LieferantenNr);



